        <synonyms>can be reached</synonyms>
        <synonyms>touchable</synonyms>
        <synonyms>attainable</synonyms>
        <synonyms>accessible</synonyms>
        <synonyms>achievable</synonyms>
        <synonyms>within reach</synonyms>
        <synonyms>graspable</synonyms>
        <synonyms>reach the tap</synonyms><synonyms>reach the water dispenser</synonyms><synonyms>approachable</synonyms>
        <pattern>can.*(touch.*?|attain.*?|access.*?|achiev.*?|reach.*?|grasp.*?|approach.*?)</pattern>
    </condition>
</root>